config_version=5

[application]

config/name="Squash the Creeps"
config/description="Squash the Creeps is a freeware game developed in the Godot Engine where you control a squid-like creature and squash incoming enemies.

It is based in the isometric 3D game of the same name implemented in the tutorial Create Your First Complete 3D Game with Godot by GDQuest."
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://assets/icon.png"

[autoload]

Configuration="*res://autoload/configuration.gd"
GameState="*res://autoload/game_state.tscn"
Hotkeys="*res://autoload/hotkeys.gd"
Utils="*res://autoload/utils.gd"
ResourceQueue="*res://autoload/resource_queue.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[editor_plugins]

enabled=PackedStringArray()

[gdnative]

singletons=[]

[global]

window=false
LAYER=false
grid=false

[gui]

theme/custom="res://ui_theme.tres"
theme/custom_font="res://assets/fonts/montserrat_semi_bold_24.tres"

